{
  "name": "@aloshy-ai/starter",
  "description": "A starter template for Next.js with Supabase",
  "repository": "https://github.com/aloshy-ai/starter.git",
  "version": "0.1.0",
  "private": false,
  "license": "MIT",
  "scripts": {
    "dev": "yarn db:start && next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "lint:fix": "next lint --fix",
    "format:ts": "prettier --write .",
    "test": "jest --passWithNoTests && playwright test",
    "format:sql": "for file in supabase/**/*.sql; do sql-formatter -l postgresql \"$file\" -o \"$file\"; done",
    "format": "yarn format:ts && yarn format:sql",
    "db:start": "supabase start",
    "db:stop": "supabase stop",
    "db:reset": "supabase db reset",
    "db:push": "supabase db push",
    "db:types": "supabase gen types typescript --local > src/lib/supabase/types.ts",
    "db:migrate:new": "read -p \"Migration name: \" name && supabase migration new $name",
    "db:status": "supabase status",
    "db:studio": "open http://localhost:54323",
    "db:lint": "plpgsql_check",
    "test:e2e": "playwright test",
    "test:unit": "jest",
    "test:watch": "jest --watch",
    "clean": "rm -rf .next out",
    "type-check": "tsc --noEmit",
    "prepare": "husky install"
  },
  "dependencies": {
    "@radix-ui/react-slot": "^1.1.0",
    "@supabase/ssr": "^0.5.2",
    "@supabase/supabase-js": "^2.46.2",
    "@vercel/og": "^0.6.4",
    "button": "^1.1.1",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "framer-motion": "^11.13.1",
    "lucide-react": "^0.464.0",
    "next": "15.0.3",
    "next-themes": "^0.4.4",
    "react": "19.0.0-rc-66855b96-20241106",
    "react-dom": "19.0.0-rc-66855b96-20241106",
    "shadcn": "^2.1.6",
    "tailwind-merge": "^2.5.5",
    "tailwindcss-animate": "^1.0.7",
    "ui": "^0.2.4"
  },
  "devDependencies": {
    "@playwright/test": "^1.42.0",
    "@supabase/sql-formatter": "^4.0.3",
    "@types/jest": "^29.5.14",
    "@types/node": "^20",
    "@types/react": "^18",
    "@types/react-dom": "^18",
    "@types/supertest": "^6.0.2",
    "autoprefixer": "^10.4.20",
    "cssnano": "^7.0.6",
    "eslint": "^8",
    "eslint-config-next": "15.0.3",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.2.1",
    "husky": "^8.0.0",
    "jest": "^29.7.0",
    "node-mocks-http": "^1.16.1",
    "playwright": "^1.49.0",
    "postcss": "^8",
    "prettier": "^3.4.1",
    "prettier-plugin-tailwindcss": "^0.6.9",
    "supabase": "^1.226.3",
    "supertest": "^7.0.0",
    "tailwindcss": "^3.4.1",
    "ts-jest": "^29.2.5",
    "ts-node": "^10.9.2",
    "typescript": "^5",
    "vercel": "^39.1.2"
  },
  "engines": {
    "node": ">=20.0.0",
    "yarn": ">=1.22.0"
  },
  "packageManager": "yarn@1.22.0"
}
